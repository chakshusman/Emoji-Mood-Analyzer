   "source": [
    "from textblob import TextBlob\n",
    "\n",
    "def analyze_text(text):\n",
    "    blob = TextBlob(text)\n",
    "    sentiment = blob.sentiment.polarity\n",
    "    if sentiment > 0:\n",
    "        emoji = \"😄\"\n",
    "    elif sentiment < 0:\n",
    "        emoji = \"😞\"\n",
    "    else:\n",
    "        emoji = \"😐\"\n",
    "\n",
    "    return emoji\n"
   ]
